
#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main()
{
	float a,b,c,x1,x2,D;
	cout<<"Ingrese a"<<endl;
	cin>>a;
	cout<<"Ingrese b"<<endl;
	cin>>b;
	cout<<"Ingrece c"<<endl;
	cin>>c;
	if (a!=0)
		{ D=b*b-4*a*c;
        	if (D>0)
	          { x1=(-b+sqrt(D))/(2*a);
			    x2=(-b+-sqrt(D))/(2*a);
				cout<<"x1="<<x1<<endl;
				cout<<"x2="<<x2<<endl;
	
	          }
			else 
				if (D==0)
					cout<<"Solucion unica;"<<(-b)/(2*a);

				else 
					cout<<"soluciones imaginarias";
         }
	getch();
	 }
